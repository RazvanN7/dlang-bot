doctype html
head
  title Overall Top Contributors
body(style="text-align: center; position: absolute; left: 0; top: 0; height: 100%; width: 100%")
  h2(style="margin-top: 6rem") Overall Top Contributors

  table(style="margin-left: auto; margin-right: auto")
    - foreach (entry; entries)
      tr
        - foreach(value; entry)
          td= value

  p * Foundation member - does not qualifiy for receiving prizes
